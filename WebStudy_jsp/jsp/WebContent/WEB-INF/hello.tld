<?xml version="1.0" encoding="UTF-8"?>
<taglib version="2.0" xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xml="http://www.w3.org/XML/1998/namespace" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/web-jsptaglibrary_2_0.xsd ">
  <tlib-version>1.0</tlib-version>
  <short-name>simple</short-name>
  <!-- uri : 태그 라이브러리 파일을 JSP 문서에서 불러오기 위해 선언하는 고유값  -->
  <uri>http://www.tjoeun.com/jsp/custom</uri>
  
  <!-- tag : 태그를 선언하기 위한 상위 엘리먼트 -->
  <tag>
  	<!-- name : 태그의 이름을 설정하는 엘리먼트 -->
  	<name>hello</name>
  	<!-- tag-class : 태그로 동작될 클래스를 설정하는 엘리먼트 -->
  	<tag-class>com.tjoeun.custom.HelloTag</tag-class>
  	<!-- body-content : 태그 내용을 설정하는 엘리먼트 -->
  	<body-content>empty</body-content>
  </tag>
</taglib>
